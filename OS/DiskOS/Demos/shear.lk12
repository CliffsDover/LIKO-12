['luacode'] = "\
-- Sorting Demo\
-- By ZEP\
-- Originally for PICO-8\
-- Ported to LIKO-12 by RamiLego4Game\
\
clear(1)\
cam(\"shear\",0.2,-0.1)\
cam(\"scale\",0.6,0.6)\
cam(\"translate\",40,60)\
G = {3,5,7,2,9,1,2,7,4,6,3}\
\
function draw_giraffe(x,y,l)\
 \
 -- Body\
 SpriteGroup(49,x-8,y-8,2,1)\
 \
 -- Neck for l segments\
 for i=1,l do\
  Sprite(26, x,y-8-i*8)\
 end\
 \
 -- Put head on top\
 Sprite(2,x,y-16-l*8)\
end\
\
function update()\
 rect(1,1,192,128,false,13)\
 rect(1,101,192,28,false,4)\
 printCursor(1,1,0)\
 color(8)\
 print(\"Press Up to randomize\")\
 print(\"Press Down to sort\")\
 \
 for i=1, #G do\
  draw_giraffe(i*16+1,101,G[i])\
 end\
end\
\
function keypressed(key,isrepeat)\
 \
 -- Down to sort\
 if key == \"down\" then\
  for i=1, (#G-1) do\
   \
   if (G[i] > G[i+1]) then\
   \
    -- The left one is taller,\
    -- So swap them !\
    local temp = G[i]\
    G[i] = G[i+1]\
    G[i+1] = temp\
    \
    -- Just one swap for now!\
    break\
   end\
  end\
 end\
 \
 -- Up to randomise\
 if key == \"up\" then\
  \
  for i=1,#G do\
   G[i] = math.floor(math.random(9))\
  end\
  \
 end\
end\
\
--Show the onscreen keyboard for android users.\
function touchpressed()\
 textinput(true)\
end\
\
clear(1)\
eventLoop()",
